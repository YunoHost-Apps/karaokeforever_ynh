{
    "name": "Karaoke Eternal",
    "id": "karaokeforever_ynh",
    "packaging_format": 1,
    "description": {
        "en": "Host awesome karaoke parties where everyone can easily find and queue songs from their phone's browser.",
        "fr": "Hébergez et gérez facilement les playlists de vos karaokés facilement à plusieurs depuis vos smartphones."
    },
    "version": "1.0~ynh1",
    "url": "https://www.karaoke-eternal.com ",
    "upstream": {
        "license": "ISC",
        "website": "https://www.karaoke-eternal.com",
        "demo": "",
        "admindoc": "https://www.karaoke-eternal.com/docs/",
        "userdoc": "https://www.karaoke-eternal.com/",
        "code": "https://github.com/bhj/KaraokeEternal/"
    },
    "license": "ISC",
    "maintainer": {
        "name": "oiseauroch",
        "email": "tobias.ollive@oiseauroch.fr"
    },
    "requirements": {
        "yunohost": ">= 11.0.0"
    },
    "multi_instance": true,
    "services": [
        "npm",
        "node"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
